{
  "name": "quickstart-james-kam-radcliffe",
  "description": "",
  "version": "6.0.2",
  "main": "index.js",
  "author": {
    "name": "James Kam-Radcliffe",
    "url": "https://quickstart-james-kam-radcliffe.myshopify.com"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "scripts": {
    "login": "shopify logout && shopify login --store https://swanky-impact-sandbox.myshopify.com && shopify theme serve",
    "pull": "shopify theme pull --nodelete",
    "dev": "npm run build && npm-run-all --parallel dev:*",
    "dev:shopify": "",
    "dev:rollup": " rollup -c --watch",
    "dev:postcss": " postcss src/styles/{theme.css,sections/*.css,components/*.css} --dir assets --ext 'bundle.css' --watch",
    "serve": "shopify theme dev --store https://probreeze-uk.myshopify.com",
    "dev3": "npm run build && npm-run-all --parallel dev3:*",
    "dev3:rollup": " rollup -c --watch",
    "dev3:postcss": " postcss src/styles/{theme.css,sections/*.css,components/*.css} --dir assets --ext 'bundle.css' --watch",
    "dev_tailwind:postcss": " TAILWIND_MODE=watch postcss src/styles/{theme.css,sections/*.css,components/*.css} --dir assets --ext 'bundle.css' --watch",
    "build": "npm-run-all --sequential build:*",
    "build:rollup": " rollup -c",
    "build:postcss": "postcss src/styles/{theme.css,sections/*.css,components/*.css} --dir assets --ext 'bundle.css'",
    "deploy": "npm-run-all --sequential deploy:*",
    "deploy:build": "npm-run-all --sequential build:*",
    "deploy:shopify": "shopify theme push --development",
    "package": "npm-run-all --sequential package:*",
    "package:build": "npm-run-all --sequential build:*",
    "package:shopify": "shopify theme package",
    "package:copy": "mkdir -p build && cpy './*.zip' build",
    "sass": "node-sass -w src/styles -o ./assets --output-style compressed"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@jonathanmoore/rollup-plugin-copy": "^3.5.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@shopify/prettier-plugin-liquid": "^1.0.0-rc.0",
    "@shopify/stylelint-plugin": "^10.1.2",
    "autoprefixer": "^10.4.7",
    "chokidar": "^3.5.2",
    "colorette": "^1.2.2",
    "cpy-cli": "^4.2.0",
    "eslint": "^7.29.0",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "is-plain-object": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.2",
    "postcss-for": "^2.1.1",
    "postcss-import": "^14.0.2",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-nested": "^5.0.5",
    "postcss-partial-import": "^2.1.0",
    "rollup": "^2.52.4",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-watch": "^3.2.2",
    "swiper": "^8.3.2",
    "tailwindcss": "^2.2.4"
  },
  "dependencies": {
    "canvas-confetti": "^1.4.0",
    "node-sass": "^8.0.0",
    "parallel": "^1.2.0",
    "prettier": "^2.8.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0"
  }
}
